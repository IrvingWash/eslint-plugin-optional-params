{
	"name": "eslint-plugin-optional-params",
	"version": "0.0.1",
	"description": "A ESLint plugin for linting optional parameters",
	"main": "dist/index.js",
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/IrvingWash/eslint-plugin-optional-params.git"
	},
	"scripts": {
		"lint": "eslint \"src/**/*\" --max-warnings=0",
		"check-ts": "tsc --noEmit",
		"build-ts": "rm -rf dist && tsc --project tsconfig.build.json",
		"build-es": "node ./esbuild.mjs",
		"prepare": "husky install"
	},
	"author": "WI",
	"license": "ISC",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"@typescript-eslint/utils": "^5.61.0",
		"esbuild": "^0.18.17",
		"eslint": "^8.39.0",
		"husky": "^8.0.0",
		"typescript": "^5.1.6"
	}
}
